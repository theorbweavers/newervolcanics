{
  "name": "newer-volcanics",
  "description": "Newer Volcanics",
  "version": "1.0.0",
  "author": "Stuart Flanagan <stuart.flanagan@gmail.com>",
  "dependencies": {
    "@sindresorhus/class-names": "^0.1.0",
    "gatsby": "^2.0.0",
    "gatsby-image": "^2.0.12",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-source-contentful": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.3",
    "little-loader": "^0.2.0",
    "postcss-preset-env": "^6.0.7",
    "react": "^16.5.1",
    "react-avatar": "^3.4.6",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-pose": "^3.3.6"
  },
  "keywords": [
    "The Orbweavers",
    "Gatsby"
  ],
  "config": {
    "newervolcanics": {
      "bucketName": "orb-newervolcanics",
      "directory": "www"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "publish": "gatsby build && npm run deploy:dev",
    "deploy:dev": "surge --domain newer-volcanics.surge.sh ./public/",
    "deploy:prod": "aws s3 cp ./public/ s3://$npm_package_config_newervolcanics_bucketName/$npm_package_config_newervolcanics_directory/ --recursive --profile sf --region ap-southeast-2 --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "jsonwebtoken": "^8.3.0",
    "prettier": "^1.14.2",
    "dotenv": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
